<?xml version="1.0" encoding="UTF-8"?>
<project name="Ghost4J">
	<version />
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.0</version>
	</skin>
	<bannerLeft>
		<name></name>
	</bannerLeft>
	<custom>
		<fluidoSkin>
			<topBarIcon>
				<name>Ghost4J</name>
				<alt>Ghost4J</alt>
				<src>/images/topbar-logo.png</src>
				<href>/index.html</href>
			</topBarIcon>
			<topBarEnabled>true</topBarEnabled>
			<sideBarEnabled>true</sideBarEnabled>
			<sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
			<facebookLike />
			<googlePlusOne />
			<gitHub>
				<projectId>zippy1978/ghost4j</projectId>
				<ribbonOrientation>right</ribbonOrientation>
			</gitHub>
		</fluidoSkin>
	</custom>
	<body>

		<head>
			<script type="text/javascript">

				var _gaq = _gaq || [];
				_gaq.push(['_setAccount',
				'UA-18388403-8']);
				_gaq.push(['_trackPageview']);

				(function() {
				var ga
				= document.createElement('script'); ga.type = 'text/javascript';
				ga.async = true;
				ga.src = ('https:' == document.location.protocol ?
				'https://ssl' :
				'http://www') + '.google-analytics.com/ga.js';
				var s =
				document.getElementsByTagName('script')[0];
				s.parentNode.insertBefore(ga, s);
				})();
				
				$(document).ready(function(){
					$('.dlLink').click(function(){
						_gaq.push(['_trackEvent', 'Download', this.text]);
					});
				});

			</script>
		</head>

